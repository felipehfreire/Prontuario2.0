<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="~/Images/favicon.ico" type="image/x-icon">
    <title>Prontuário | @ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
     @Scripts.Render("~/bundles/ie")
     <![endif]-->
    <script src="@Url.Content("~/Scripts/moment.min.js")" type="text/javascript"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/plugins/navgoco")
    @Scripts.Render("~/plugins/pace")
    @Scripts.Render("~/bundles/app")
    @Scripts.Render("~/bundles/moment")
    @Scripts.Render("~/plugins/select2")
    


    @RenderSection("scripts", required: false)
    
    <script src="~/scripts/plugins/messenger/messenger-theme-flat.js"></script>
    <script src="~/scripts/plugins/messenger/messenger.min.js"></script>

</head>
<body>
    <!-- Page Wrapper-->
    <section id="main-wrapper" class="theme-default">

        <!-- Top Header -->
        @Html.Partial("_Header")

        <!-- Left Sidebar Navigation -->
        @Html.Partial("_LeftSideBar")

        <!-- Content wraper -->
        <section class="main-content-wrapper">
            <!-- Main view  -->
            @RenderBody()
        </section>
        <!-- /Content wraper-->

    </section><!-- /Page Wrapper-->
    <!-- Right Sidebar -->
    @Html.Partial("_RightSideBar")

    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/plugins/navgoco")
    @Scripts.Render("~/plugins/pace")
    @Scripts.Render("~/bundles/app")
    @Scripts.Render("~/bundles/fullcalendar")
    @Scripts.Render("~/bundles/moment")
    
    @RenderSection("scripts", required: false)*@
    

</body>
</html>

